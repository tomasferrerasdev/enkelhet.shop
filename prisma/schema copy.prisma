generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum SeriesType {
  lounge
  alabaster
  capsule
}

enum FinishType {
  Oak
  Ash
  Walnut
  Wenge
}

model Series {
  id String @id @default(uuid())
  name String @unique
  // ???
}

model Product {
    id String @id @default(uuid())
    description String
    inStock Int
    price Float @default(0)
    finish FinishType
    series SeriesType
    slug String @unique
    tags String[] @default([])
    title String
    measurement String
}